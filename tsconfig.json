{
	"compilerOptions": {
		"target": "ES2015",
		"module": "commonjs",
		"sourceMap": true,
		"inlineSources": true,
		"declaration": true,
		"outDir": "./dist",
		"rootDir": "./src",
		"strict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noFallthroughCasesInSwitch": true,
		"moduleResolution": "node",
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true
	},
	"exclude": ["**/*.fixture.ts"]
}
