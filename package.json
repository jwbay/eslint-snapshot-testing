{
  "name": "eslint-rule-snapshot-testing",
  "version": "0.0.0",
  "description": "Fixture-based Jest snapshot testing for ESLint rules",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch --preserveWatchOutput",
    "lint": "prettier . --check",
    "format": "prettier . --write",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:jwbay/eslint-snapshot-testing.git"
  },
  "keywords": [
    "eslint",
    "rules",
    "snapshot",
    "test",
    "fixture"
  ],
  "author": "justin.bay@outlook.com",
  "license": "MIT",
  "peerDependencies": {
    "eslint": "^7.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@types/eslint": "^7.2.0",
    "@types/estree": "^0.0.45",
    "@types/jest": "^26.0.8",
    "@types/node": "^14.0.27",
    "eslint": "^7.6.0",
    "jest": "^26.2.2",
    "prettier": "^2.0.5",
    "semantic-release": "^17.1.1",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "comment-parser": "^0.7.5"
  }
}
